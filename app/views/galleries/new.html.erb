<h1>New gallery</h1>

<%= error_messages_for :gallery %>

<% form_for(@gallery) do |f| %>
  <p>
    <b>File name</b><br />
    <%= f.text_field :file_name %>
  </p>

  <p>
    <b>Description</b><br />
    <%= f.text_field :description %>
  </p>

  <p>
    <b>Belonging</b><br />
    <%= f.text_field :belonging_id %>
  </p>

  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', galleries_path %>
